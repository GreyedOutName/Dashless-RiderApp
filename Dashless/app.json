{
  "expo": {
    "name": "Dashless",
    "slug": "Dashless",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugin":[
      [
        "expo-notifications",
        {
          "icon": "./assets/DashlessLogo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
        }
      ]
    ],
    "splash": {
      "image": "./assets/DashlessLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/DashlessLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
